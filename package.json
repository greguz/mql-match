{
  "name": "mql-match",
  "version": "0.4.0",
  "description": "MongoDB Query Language match function compiler",
  "type": "module",
  "main": "mql-match.cjs",
  "types": "mql-match.d.ts",
  "files": [
    "!**/*.spec.mjs",
    "lib",
    "mql-match.cjs",
    "mql-match.d.ts",
    "mql-match.mjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/mql-match.git"
  },
  "scripts": {
    "lint": "standard",
    "build": "rollup -c rollup.config.js",
    "test": "ava",
    "prepublishOnly": "npm run lint && npm run build && npm run test"
  },
  "keywords": [
    "mongo",
    "mongodb",
    "mql",
    "query",
    "filter",
    "compile"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "devDependencies": {
    "ava": "^4.3.3",
    "bson": "^4.7.0",
    "rollup": "^2.78.1",
    "standard": "^17.0.0"
  },
  "ava": {
    "extensions": [
      "mjs"
    ],
    "files": [
      "**/*.spec.mjs"
    ]
  }
}
