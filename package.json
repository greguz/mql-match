{
  "name": "mql-match",
  "version": "0.7.1",
  "description": "A MongoDB Query Language compiler.",
  "main": "./mql-match.cjs",
  "types": "./mql-match.d.ts",
  "type": "module",
  "exports": {
    "import": "./mql-match.mjs",
    "require": "./mql-match.cjs",
    "types": "./mql-match.d.ts"
  },
  "files": [
    "!**/*.spec.mjs",
    "lib",
    "mql-match.cjs",
    "mql-match.d.ts",
    "mql-match.mjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/mql-match.git"
  },
  "bugs": {
    "url": "https://github.com/greguz/mql-match/issues"
  },
  "scripts": {
    "lint": "standard",
    "build": "rollup -c rollup.config.js",
    "test": "c8 ava",
    "prepublishOnly": "npm run lint && npm run build && npm run test"
  },
  "keywords": [
    "mongo",
    "mongodb",
    "mql",
    "query",
    "filter",
    "compile"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "dependencies": {
    "bson": "^6.8.0",
    "buffer": "^6.0.3",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "ava": "^6.1.3",
    "c8": "^10.1.2",
    "rollup": "^4.20.0",
    "standard": "^17.1.0"
  },
  "ava": {
    "extensions": [
      "mjs"
    ],
    "files": [
      "**/*.spec.mjs"
    ]
  }
}
