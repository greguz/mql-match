{
  "name": "mql-match",
  "version": "0.7.1",
  "description": "A MongoDB Query Language compiler.",
  "main": "./mql-match.cjs",
  "types": "./mql-match.d.ts",
  "type": "module",
  "exports": {
    "import": "./mql-match.mjs",
    "require": "./mql-match.cjs",
    "types": "./mql-match.d.ts"
  },
  "files": [
    "!**/*.spec.mjs",
    "lib",
    "mql-match.cjs",
    "mql-match.d.ts",
    "mql-match.mjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/mql-match.git"
  },
  "bugs": {
    "url": "https://github.com/greguz/mql-match/issues"
  },
  "scripts": {
    "lint": "standard",
    "build": "rollup -c rollup.config.js",
    "test": "c8 ava",
    "prepublishOnly": "npm run lint && npm run build && npm run test"
  },
  "keywords": [
    "mongo",
    "mongodb",
    "mql",
    "query",
    "filter",
    "compile"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "dependencies": {
    "bson": "^6.10.4",
    "decimal.js": "^10.6.0"
  },
  "devDependencies": {
    "@ava/typescript": "^6.0.0",
    "@biomejs/biome": "2.2.4",
    "ava": "^6.4.1",
    "c8": "^10.1.3",
    "typescript": "^5.9.3"
  },
  "ava": {
    "files": [
      "**/*.spec.ts"
    ],
    "typescript": {
      "rewritePaths": {
        "src/": "build/"
      },
      "compile": "tsc"
    }
  }
}
